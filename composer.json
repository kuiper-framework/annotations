{
    "name": "kuiper/annotations",
    "type": "library",
    "description": "Annotations parser component",
    "keywords": ["annotations"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "wenbinye@163.com"
        }
    ],
    "require": {
        "php": ">= 5.6",
        "kuiper/reflection": "~0.2",
        "doctrine/lexer": "^1.0",
        "psr/log": "^1.0",
        "symfony/event-dispatcher": "^3.1"
    },
    "suggest": {
        "monolog/monolog": "For logging",
        "kuiper/cache": "For cache annotations"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "winwin/composer-phpcs-plugin": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\annotations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\annotations\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3-dev"
        }
    }
}
